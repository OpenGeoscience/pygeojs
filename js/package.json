{
  "name": "@johnkit/pygeojs",
  "version": "0.1.1",
  "description": "ipywidget wrapper for GeoJS",
  "author": "john Tourtellott",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//pygeojs.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "src/*.js",
    "dist/*.js",
    "scripts/*"
  ],
  "scripts": {
    "autogen": "node ./scripts/generate-wrappers.js",
    "clean": "rimraf dist/",
    "postinstall": "npm run autogen",
    "prepublish": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "fs-extra": "^7.0.0",
    "handlebars": "^4.0.12",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.0 || 2",
    "geojs": "^0.18.1",
    "jupyter-dataserializers": "^1.3.0",
    "lodash": "^4.17.4"
  },
  "jupyterlab": {
    "extension": "src/labplugin"
  }
}
